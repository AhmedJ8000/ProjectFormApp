@using ProjectWebApp.ViewModel;
@model NewLogViewModel;
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Administration</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.log.Table)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.log.Status)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.log.LDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.log.Time)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.log.User)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Logs) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Table)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Time)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.User.Id)
            </td>
        </tr>
}
    </tbody>
</table>
